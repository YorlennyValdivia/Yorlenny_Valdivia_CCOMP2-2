//Los factores primos de 13195 son 5, 7, 13 y 29. ¿Cuál es el mayor factor primo del número 600851475143?
#include <iostream>
#include <cmath>
using namespace std;

int main() {
    long int n = 600851475143;
    int max= 0;
    while (n % 2 == 0) {
        max= 2;
        n /= 2;
    }
    for (int i = 3; i <= sqrt(n); i += 2){
        while (n % i == 0) {
            max = i;
            n /= i;
        }
    }
     if (n > 2) {
        max = n;
    }

    cout << "factor primo es: " << max << endl;
    return 0;
}
